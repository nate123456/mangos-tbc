page "/scripts/{token:string}"
@*@using PlayerbotWeb.Services
@using PlayerbotWeb.Models

<PageTitle>Scripts</PageTitle>
@inject PlayerbotRepository _playerbotRepository*@

<h1>Scripts</h1>

<p>Below are all the current scripts.</p>

@*
<table class="table">
    <thead>
    <tr>
        <th>Name</th>
        <th>Script</th>
    </tr>
    </thead>
    <tbody>
    @foreach (var script in StoredScripts)
    {
        <tr>
            <td>@script.Name</td>
            <td>@script.Script</td>
        </tr>
    }
    </tbody>
</table>

@code {

    [Parameter]
    public string? Token { get; set; } = "something";

    public IEnumerable<PlayerbotScript> StoredScripts = new List<PlayerbotScript>();

    protected override async Task OnInitializedAsync()
    {
        if (Token != null) StoredScripts = await _playerbotRepository.GetScriptsAsync(Token.ToUpper());
    }

}*@